{
    "name": "ayimdomnic/quicksite",
    "description": "A CMS to assit you set up; events, blogs, pages,images etc on any laravel app",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "odhiambo dormnic",
            "email": "ayimdomnic@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.3",
        "yab/laracogs": "^2.0",
        "devfactory/minify": "1.0.*",
        "spatie/laravel-analytics": "^1.4",
        "aws/aws-sdk-php": "^3.18",
        "league/flysystem-aws-s3-v3": "^1.0",
        "graham-campbell/markdown": "^6.1",
        "intervention/image": "^2.3"
    },

    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "mikey179/vfsStream": "^1.6",
        "orchestra/testbench": "~3.0"
        
    },
    "autoload": {
        
        "psr-4": {
            "Ayimdomnic\\QuickSite\\": "src/"
        },

        "files": [
          "src/Helpers/blade.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
